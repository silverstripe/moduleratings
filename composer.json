{
    "name": "silverstripe/moduleratings",
    "description": "A library to provide module code quality ratings",
    "type": "library",
    "require": {
        "symfony/console": "~3.0|~4.0",
        "symfony/finder": "~3.0|~4.0",
        "symfony/yaml": "~3.0|~4.0",
        "psr/log": "^1.0",
        "guzzlehttp/guzzle": "^6"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Robbie Averill",
            "email": "robbie@silverstripe.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "SilverStripe\\ModuleRatings\\": "src/",
            "SilverStripe\\ModuleRatings\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}
